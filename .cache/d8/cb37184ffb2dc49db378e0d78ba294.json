        this.input = c;\n                     }}/>\n                ) : (\n                    <span className=\"note__text\">{note.text} </span>\n                )}\n\n                {isEditing && (\n                    <React.Fragment>\n                        <button\n                            className=\"note__button note__button--red\"\n                            onClick={this.handleCancel} \n                        >\n                            <i className=\"material-icons\">cancel</i>\n                        </button>                    \n                        <button\n                            className=\"note__button  note__button--green\"\n                            onClick={this.handleSave} \n                        >\n                            <i className=\"material-icons\">done_outline</i>\n                        </button>\n                    </React.Fragment>\n                )}\n                <button\n                    disabled={isEditing}\n                    className=\"note__button\"\n                    onClick={this.handleEdit}\n                >\n                    <i className=\"material-icons\">edit</i>\n                </button>\n                <button\n                    disabled={isEditing}\n                    className=\"note__button\"\n                    onClick={() => {\n                        onDelete(note.id);\n                        }} \n                >\n                    <i className=\"material-icons\">delete</i>\n                </button>\n                <button\n                    className={classNames(\"note__button\", {\n                        \"note__button--hidden\": index === 0\n                    })}\n                    onClick={() => {\n                        onMove(\"up\", index);\n                        }} \n                >\n                    <i className=\"material-icons\">arrow_upward</i>\n                </button>\n                \n                <button\n                    className={classNames(\"note__button\", {\n                        \"note__button--hidden\": index === total - 1\n                    })}\n                    onClick={() => {\n                        onMove(\"down\", index);\n                    }} \n                >\n                <i className=\"material-icons\">arrow_downward</i>\n                </button>\n            </div>\n        );\n    }\n\n}\n\nexport default Note;"},"lineCount":null}},"error":null,"hash":"45caf6fdc03c481fa3ba203d356b8492","cacheData":{"env":{}}}